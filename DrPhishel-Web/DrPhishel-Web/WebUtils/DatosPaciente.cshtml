<form role="form" class="center-block login_panel">
    <div>
        <h2>Datos personales</h2>
        <div class="form-group">
            <label for="nombre">Nombre:</label>
            <input maxlength="20" readonly="readonly" class="form-control" id="nombre" ng-model="usuario_Nombre">
        </div>
        <div class="form-group">
            <label for="primer_apellido">Primer Apellido:</label>
            <input maxlength="20" readonly="readonly" class="form-control" id="primer_apellido" ng-model="usuario_Primer_apellido">
        </div>
        <div class="form-group">
            <label for="segundo_apellido">Segundo Apellido:</label>
            <input maxlength="20" readonly="readonly" class="form-control" id="segundo_apellido" ng-model="usuario_Segundo_apellido">
        </div>

        <div class="form-group">
            <label for="fecha_nacimiento">Fecha de Nacimiento:</label>
            <input type="date" readonly="readonly" class="form-control" id="fecha_nacimiento" ng-model="usuario_Fecha_nacimiento">
        </div>
        <div class="form-group">
            <label for="telefono">N&uacute;mero telefonico:</label>
            <input pattern="[1-9][0-9]*"  readonly="readonly" class="form-control" id="telefono" ng-model="usuario_Telefono">
        </div>
        <div class="form-group">
            <label for="residencia">Residencia:</label>
            <input maxlength="100"  readonly="readonly" class="form-control" id="recidencia" ng-model="usuario_Residencia">
        </div>
        <input class="login_component btn btn-warning center-block" id="registroPaciente" type="button" value="Registrar como paciente" />
    </div>
</form>